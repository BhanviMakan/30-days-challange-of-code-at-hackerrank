#include <math.h>
#include <stdio.h>
#include <stdlib.h>
#include <string.h>
int main(){
    int n,a[1000],b[1000],i,j;
    scanf("%d",&n);
    for(i=0;i<n;i++){
        scanf(" %d",&a[i]);
    }
    for(i=n-1,j=0;i>=0;i--,j++){
        b[j]=a[i];
    }
    for(i=0;i<n;i++){
        printf("%d ",b[i]);
    }
    return 0;
}
